<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@ taglib prefix="c" uri="jakarta.tags.core" %>

<div class="card">

	<div class="card-header">
		부럽지?
	</div>
	<div class="card-body">
	<c:forEach var="envy" items="${envies}">
		<div class="row">
			<div class="d-none">${envy.post_id }</div>
				<div class="col-5">${envy.title }</div>
			<div class="col row">
				<div class="col"><span class="pe-2">♥️</span>${envy.like_point }</div>
				<div class="col"><span class="pe-2">🗨️</span>${envy.comment_cnt }</div>
			</div>
		</div>
	</c:forEach>
	</div>

</div>